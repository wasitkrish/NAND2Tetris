CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux(in=in, sel=sel[2],a=z1,b=z2);
    DMux(in=z1, sel=sel[1],a=o1,b=o2);
    DMux(in=z2, sel=sel[1],a=o3,b=o4);
    DMux(in=o1 , sel=sel[0] , a=a , b=b );
    DMux(in=o2 , sel=sel[0] , a=c , b=d );
    DMux(in=o3 , sel=sel[0] , a=e , b=f );
    DMux(in=o4 , sel=sel[0] , a=g , b=h );


}