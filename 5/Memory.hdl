CHIP Memory {
    IN in[16], load, address[15];
    OUT out[16];

    PARTS:
    DMux4Way(in=load,sel=address[13..14],a=loadram,b=loadram2,c=loadscreen,d=loadkbd);
    Or(a=loadram,b=loadram2,out=loadramc);
    RAM16K(in=in,load=loadramc,address=address[0..13],out=rout);
    Screen(in=in,load=loadscreen,address=address[0..12],out=sout);
    Keyboard(out=outkbd);
    Mux4Way16(a=rout,b=rout,c=sout,d=outkbd,sel=address[13..14],out=out);


}